{
  "name": "parcel-vue-single-html-page",
  "version": "1.0.0",
  "description": "A parcel vue single html page",
  "scripts": {
    "dev": "parcel src/*.html",
    "build": "parcel build ./src/*.html --no-minify",
    "github-action-build": "parcel build ./src/*.html --public-url .",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "less": "^3.11.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "postcss-pxtorem": "^5.1.1"
  },
  "browserslist": [
    "last 3 years"
  ]
}